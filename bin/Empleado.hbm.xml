<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate mapping DTD 5.3//EN"
 "http://Hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
 
 <hibernate-mapping>
 <class name="es.centroafuera.ad.hibernate.ejemplo1.Empleado"
 table="empleado">
	<id name="emp_no" column="emp_no">
		<generator class="increment"></generator>
	</id>
	<property name="nombre" column="nombre" type="string"></property> 
 	<property name="apellido" column="apellido" type="string"></property> 
 	<property name="oficio" column="oficio" type="string"></property> 
 	<property name="dir" column="dir" type="int"></property> 
 	<property name="fecha_alt" column="fecha_alt" type="timestamp"></property> 
 	<property name="salario" column="salario" type="float" ></property> 
 	<property name="comision" column="comision" type="float"></property> 
 	<property name="dept_no" column="dept_no" type="int"></property> 
 </class>
 
 </hibernate-mapping>